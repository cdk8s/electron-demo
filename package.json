{
  "name": "jeemoo-gitee",
  "version": "1.0.0",
  "description": "积木网",
  "main": "main.js",
  "scripts": {
    "start": "electron ."
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "CDK8S",
  "license": "GUN",
  "build": {
    "productName": "jeemoo-gitee",
    "appId": "com.jeemoo.gitee",
    "asar": true,
    "compression": "normal",
    "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
    "directories": {
      "output": "./build"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "devDependencies": {
    "electron": "^9.1.1",
    "electron-builder": "21.2.0",
    "electron-rebuild": "^1.11.0"
  }
}
